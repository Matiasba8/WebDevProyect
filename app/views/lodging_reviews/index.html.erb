<p id="notice"><%= notice %></p>

<h1>Lodging Reviews</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lodging_reviews.each do |lodging_review| %>
      <tr>
        <td><%= link_to 'Show', lodging_review %></td>
        <td><%= link_to 'Edit', edit_lodging_review_path(lodging_review) %></td>
        <td><%= link_to 'Destroy', lodging_review, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

